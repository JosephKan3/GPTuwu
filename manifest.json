{
  "manifest_version": 3,
  "name": "GPTuwu",
  "description": "This extension replaces the ChatGPT icon with a GIF.",
  "version": "1.0",
  "permissions": ["activeTab"],
  "web_accessible_resources": [
    { "resources": ["uwu.gif"], "matches": ["*://chat.openai.com/*"] }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
