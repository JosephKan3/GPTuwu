{
  "manifest_version": 3,
  "name": "GPTuwu",
  "description": "This extension replaces the ChatGPT icon with a GIF of a silly cat.",
  "version": "1.0",
  "web_accessible_resources": [
    {
      "resources": ["uwu.gif"],
      "matches": ["*://chat.openai.com/*"]
    }
  ],
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "32": "icon.png",
    "512": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://chat.openai.com/*"],
      "js": ["content.js"]
    }
  ]
}
